        <div class="controls">
            <button id="prevBtn" onclick="changeSlide(-1)">‚¨ÖÔ∏è Anterior</button>
            <span class="slide-counter">
                <span id="currentSlide">1</span> / <span id="totalSlides">10</span>
            </span>
            <button id="nextBtn" onclick="changeSlide(1)">Pr√≥ximo ‚û°Ô∏è</button>
        </div>
    </div>

    <script>
        // Vari√°veis globais
        let currentSlideIndex = 0;
        let score = 0;
        let userName = "";
        let quizStartTime;
        let timerInterval;
        let isReviewMode = false;
        
        const totalQuestions = 10;
        const slides = document.querySelectorAll('.slide');
        const questionsStartIndex = 1;
        const resultsSlideIndex = slides.length - 1;
        const userAnswers = []; // Armazena true/false para cada quest√£o
        const userChoices = []; // Armazena a op√ß√£o escolhida (A, B, C...)
        
        // Fun√ß√£o para mostrar um slide espec√≠fico
        function showSlide(n) {
            slides.forEach(slide => slide.classList.remove('active'));
            slides[n].classList.add('active');
            currentSlideIndex = n;
            updateControls();
            updateProgressBar();
            
            if (n >= questionsStartIndex && n < resultsSlideIndex) {
                document.querySelector('.progress-bar-container').style.display = 'block';
                document.querySelector('.info-bar').style.display = 'flex';
            } else {
                document.querySelector('.progress-bar-container').style.display = 'none';
                document.querySelector('.info-bar').style.display = 'none';
            }
        }

        // Fun√ß√£o para navega√ß√£o entre slides
        function changeSlide(direction) {
            let nextIndex = currentSlideIndex + direction;
            
            if (nextIndex > totalQuestions) {
                stopTimer();
                showResults();
            } else if (nextIndex >= 0 && nextIndex < slides.length) {
                showSlide(nextIndex);
                saveState();
            }
        }

        // Fun√ß√£o para atualizar os bot√µes de controle
        function updateControls() {
            if (currentSlideIndex === 0 || currentSlideIndex === resultsSlideIndex) {
                document.getElementById('prevBtn').style.display = 'none';
                document.getElementById('nextBtn').style.display = 'none';
                document.querySelector('.slide-counter').style.display = 'none';
            } else {
                document.getElementById('prevBtn').style.display = 'block';
                document.getElementById('nextBtn').style.display = 'block';
                document.querySelector('.slide-counter').style.display = 'block';

                document.getElementById('currentSlide').textContent = currentSlideIndex;
                document.getElementById('totalSlides').textContent = totalQuestions;
            }
            
            if (isReviewMode) {
                document.getElementById('prevBtn').style.display = 'none';
                document.getElementById('nextBtn').style.display = 'none';
            } else {
                document.getElementById('prevBtn').disabled = (currentSlideIndex === questionsStartIndex);
                document.getElementById('nextBtn').disabled = (userAnswers[currentSlideIndex - questionsStartIndex] === undefined);
            }
        }

        // Fun√ß√£o para atualizar a barra de progresso
        function updateProgressBar() {
            const progress = ((currentSlideIndex - questionsStartIndex) / totalQuestions) * 100;
            document.getElementById('progressBar').style.width = `${progress}%`;
        }
        
        // Fun√ß√£o para iniciar o cron√¥metro
        function startTimer() {
            quizStartTime = Date.now();
            timerInterval = setInterval(function() {
                const elapsedTime = Date.now() - quizStartTime;
                const totalSeconds = Math.floor(elapsedTime / 1000);
                const minutes = Math.floor(totalSeconds / 60);
                const seconds = totalSeconds % 60;
                document.getElementById('timer').textContent = `${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
            }, 1000);
        }
        
        // Fun√ß√£o para parar o cron√¥metro
        function stopTimer() {
            clearInterval(timerInterval);
        }

        // Fun√ß√£o para mostrar os resultados
        function showResults() {
            const totalSeconds = Math.floor((Date.now() - quizStartTime) / 1000);
            const minutes = Math.floor(totalSeconds / 60);
            const seconds = totalSeconds % 60;
            document.getElementById('totalTime').textContent = `${minutes} minutos e ${seconds} segundos`;
            
            document.getElementById('resultName').textContent = `Parab√©ns, ${userName}!`;
            document.getElementById('resultScore').textContent = `Sua pontua√ß√£o foi: ${score} de ${totalQuestions}`;

            let message = "";
            if (score === totalQuestions) {
                message = "Excelente! Voc√™ acertou todas as quest√µes!";
            } else if (score >= totalQuestions * 0.7) {
                message = "Muito bem! Voc√™ teve um √≥timo desempenho.";
            } else if (score >= totalQuestions * 0.5) {
                message = "Bom trabalho! Continue praticando para melhorar.";
            } else {
                message = "Voc√™ pode melhorar! Estude mais e tente novamente.";
            }
            document.getElementById('resultMessage').textContent = message;

            const resultsList = document.getElementById('resultsList');
            resultsList.innerHTML = '';
            userAnswers.forEach((isCorrect, index) => {
                const li = document.createElement('li');
                li.textContent = `Quest√£o ${index + 1}: ${isCorrect ? 'Certo' : 'Errado'} (Sua resposta: ${userChoices[index]})`;
                li.classList.add(isCorrect ? 'correct' : 'incorrect');
                
                const reviewBtn = document.createElement('button');
                reviewBtn.textContent = 'Revisar';
                reviewBtn.style.marginLeft = '10px';
                reviewBtn.style.padding = '5px 10px';
                reviewBtn.style.background = '#8e44ad';
                reviewBtn.addEventListener('click', () => {
                    isReviewMode = true;
                    showSlide(index + questionsStartIndex);

                    // Adiciona o bot√£o de voltar ao resultado quando em modo de revis√£o
                    const controls = document.querySelector('.controls');
                    let backToResultsBtn = document.getElementById('backToResultsBtn');
                    if (!backToResultsBtn) {
                        backToResultsBtn = document.createElement('button');
                        backToResultsBtn.id = 'backToResultsBtn';
                        backToResultsBtn.textContent = 'Voltar ao Resultado';
                        backToResultsBtn.style.background = '#e67e22';
                        controls.appendChild(backToResultsBtn);
                        backToResultsBtn.addEventListener('click', () => {
                            showResults();
                        });
                    }
                    backToResultsBtn.style.display = 'block';
                });

                li.appendChild(reviewBtn);
                resultsList.appendChild(li);
            });
            
            showSlide(resultsSlideIndex);
            clearState();
        }

        // Fun√ß√£o para salvar o estado do quiz no localStorage
        function saveState() {
            const state = {
                currentSlideIndex,
                score,
                userName,
                userAnswers,
                userChoices,
                quizStartTime
            };
            localStorage.setItem('quizState', JSON.stringify(state));
        }

        // Fun√ß√£o para carregar o estado do quiz do localStorage
        function loadState() {
            const savedState = localStorage.getItem('quizState');
            if (savedState) {
                const state = JSON.parse(savedState);
                if (confirm("Foi detectado um quiz em progresso. Deseja continuar de onde parou?")) {
                    currentSlideIndex = state.currentSlideIndex;
                    score = state.score;
                    userName = state.userName;
                    userAnswers.push(...state.userAnswers);
                    userChoices.push(...state.userChoices);
                    quizStartTime = state.quizStartTime;
                    document.getElementById('userName').value = userName;
                    showSlide(currentSlideIndex);
                    startTimer();
                } else {
                    clearState();
                }
            }
        }
        
        // Fun√ß√£o para limpar o localStorage
        function clearState() {
            localStorage.removeItem('quizState');
        }

        // Fun√ß√£o para reiniciar o quiz
        function restartQuiz() {
            score = 0;
            userAnswers.length = 0;
            userChoices.length = 0;
            document.querySelectorAll('.alternative').forEach(alt => {
                alt.classList.remove('correct', 'incorrect');
                alt.style.pointerEvents = 'auto';
            });
            document.querySelectorAll('.explanation').forEach(exp => {
                exp.classList.remove('show');
            });
            showSlide(0);
            document.getElementById('userName').value = '';
            clearState();
            isReviewMode = false;
            
            const backToResultsBtn = document.getElementById('backToResultsBtn');
            if (backToResultsBtn) backToResultsBtn.style.display = 'none';
        }
        
        // Event listeners
        document.getElementById('startBtn').addEventListener('click', function() {
            userName = document.getElementById('userName').value.trim();
            if (userName) {
                startTimer();
                showSlide(questionsStartIndex);
                saveState();
            } else {
                alert("Por favor, digite seu nome para come√ßar.");
            }
        });

        document.getElementById('restartBtn').addEventListener('click', function() {
            restartQuiz();
        });
        
        document.addEventListener('click', function(e) {
            if (e.target.classList.contains('alternative') && !isReviewMode) {
                const currentSlideElement = document.querySelector('.slide.active');
                const alternatives = currentSlideElement.querySelectorAll('.alternative');
                const explanation = currentSlideElement.querySelector('.explanation');
                const selectedOption = e.target.getAttribute('data-option');
                
                if (userAnswers[currentSlideIndex - questionsStartIndex] === undefined) {
                    const isCorrect = e.target.getAttribute('data-correct') === 'true';
                    userAnswers[currentSlideIndex - questionsStartIndex] = isCorrect;
                    userChoices[currentSlideIndex - questionsStartIndex] = selectedOption;
                    
                    if (isCorrect) {
                        score++;
                    }
                    
                    alternatives.forEach(alt => alt.style.pointerEvents = 'none');
                    alternatives.forEach(alt => {
                        const altCorrect = alt.getAttribute('data-correct') === 'true';
                        if (altCorrect) {
                            alt.classList.add('correct');
                        } else {
                            alt.classList.add('incorrect');
                        }
                    });
                    
                    if (explanation) {
                        explanation.classList.add('show');
                    }
                    
                    document.getElementById('nextBtn').disabled = false;
                    saveState();
                }
            }
        });

        // Inicializa√ß√£o do quiz
        showSlide(0);
        loadState();
    </script>
</body>
</html>
